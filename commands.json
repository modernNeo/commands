{
  "commands": [
    {
      "name": "listing files in current directory with a human readable size",
      "description": ["du -sh *"],
      "exec": false
    },
    {
      "name": "list processes running on a given port",
      "description": [
        "https://linuxize.com/post/check-listening-ports-linux/",
        "lsof -i :<portNumber> | grep LISTEN",
        "ss -tunlp | grep <portNumber>"
      ],
      "exec": false
    },
    {
      "name": "open archive folders",
      "description": [
        "tar xjf file.tar.bz2 -C /opt/",
        "tar xvf file.tar.gz -C /opt/",
        "tar -xf file.tar.xz -C /opt/"
      ],
      "exec": false
    },
    {
      "name": "stop/remove all containers",
      "description": [
        "docker stop $(docker ps -a -q)",
        "docker rm $(docker ps -a -q)"
      ],
      "exec": false
    },
    {
      "name": "clean apt cache",
      "description": ["sudo apt-get clean"],
      "exec": true
    },
    {
      "name": "git",
      "description": ["-"],
      "exec": false
    },
    {
      "name": "Unmount a busy device",
      "description": [
        "umount -l /PATH/OF/BUSY-DEVICE",
        "umount -f /PATH/OF/BUSY-NFS"
      ],
      "exec": false
    },
    {
      "name": "install zipped program",
      "description": [
        "install into /opt/",
        "sudo ln -sf /opt/path/to/executable/file /usr/bin/program"
      ],
      "exec": "false"
    },
    {
      "name": "location of desktop entry",
      "description": [
	      "~/.local/share/applications/",
	      "/usr/share/applications/"
      ],
      "exec": "false"
    }
  ]
}
